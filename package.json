{
  "name": "zefyr",
  "version": "0.0.1-alpha.19",
  "private": true,
  "description": "Configurable and typesafe monkey patching for TypeScript",
  "keywords": [],
  "homepage": "https://github.com/Snowfly-T/zefyr",
  "bugs": {
    "url": "https://github.com/Snowfly-T/zefyr/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Snowfly-T/zefyr"
  },
  "license": "MIT",
  "author": "Snowflyt <gaoge011022@gmail.com>",
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.build.json && rimraf ./dist/zefyr.min.js && rimraf ./dist/zefyr.min.d.ts && rimraf ./dist/zefyr.min.d.ts.map && tsc-alias -p ./tsconfig.build.json && cpy ./dist/**/* ./build/ && rimraf dist && prettier --loglevel=silent --print-width 80 --write ./build/ && node prebuild.cjs && vite build && cpy ./dist/*.min.js ./build/ && cpy ./dist/*.min.d.ts ./build/ && rimraf dist && cpy ./package.json ./build/ && replace-in-file \"  \\\"private\\\": true,\" \"\" ./build/package.json && prettier --loglevel=silent --print-width 80 --write ./build/package.json && cpy ./README.md ./build/ && cpy ./LICENSE ./build/",
    "postbuild": "node postbuild.cjs",
    "clean": "rimraf dist build",
    "format": "prettier --write {src,test}/**/*.{json,js,jsx,ts,tsx}",
    "lint": "eslint {src,test}/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix {src,test}/**/*.{js,jsx,ts,tsx}",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:watch": "vitest",
    "test:watch-cov": "vitest --coverage"
  },
  "dependencies": {
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.9",
    "@types/ramda": "^0.29.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.3",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "fraction.js": "^4.3.4",
    "lodash-es": "^4.17.21",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.5",
    "ramda": "^0.29.0",
    "replace-in-file": "^7.0.2",
    "rimraf": "^5.0.1",
    "ts-morph": "^20.0.0",
    "ts-toolbelt": "^9.6.0",
    "tsc-alias": "^1.8.8",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2",
    "vite-plugin-linter": "^2.0.5",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2"
  }
}
